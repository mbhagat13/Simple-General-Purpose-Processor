Library ieee;
use ieee.std_logic_1164.all;

ENTITY Multiplexer8to1 IS
		PORT (w0, w1, w2, w3	   :IN STD_LOGIC_VECTOR(31 DOWNTO 0);
			s 		   :IN STD_LOGIC_VECTOR(2 Downto 0);
			f		   :OUT STD_LOGIC);
END Multiplexer8to1 ;

ARCHITECTURE Behavior OF Multiplexer8to1  IS
BEGIN
	WITH s SELECT
		f<= w0 WHEN "000",
		    w1 WHEN "001",
		    w2 WHEN "010",
		    w3 WHEN "110";
			 w4 WHEN "100";
			 w5 WHEN "101";
			 x"00000000" WHEN OTHERS;
END Behavior;
	
	